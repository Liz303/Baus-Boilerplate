{
  "name": "baus-boilerplate",
  "version": "1.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && nodemon server/index --exec babel-node",
    "postinstall": "npm run dist",
    "dist": "npm run clean && npm run webpack-prod && npm run server-prod",
    "clean": "rm -rf public/dist && mkdir public/dist",
    "webpack-prod": "webpack --config ./webpack/production.config.js --progress --profile --colors",
    "server-prod": "webpack --config ./webpack/server.config.js --progress --profile --colors --display-error-details",
    "start": "npm run dev",
    "prod": "NODE_ENV=production node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "=6.2.2",
    "npm": "=3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BausCode/Baus-Boilerplate.git"
  },
  "dependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.2",
    "html-webpack-plugin": "^2.22.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "modernizr-webpack-plugin": "^1.0.5",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redbox-react": "^1.2.10",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "nodemon": "^1.9.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-error-notification": "^0.1.6",
    "webpack-hot-middleware": "^2.12.1"
  }
}
